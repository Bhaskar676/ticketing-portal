<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                   http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.8.xsd">

    <changeSet id="drop-tables" author="assistant">
        <dropTable tableName="bookings" ifExists="true"/>
        <dropTable tableName="ticket_types" ifExists="true"/>
        <dropTable tableName="events" ifExists="true"/>
        <dropTable tableName="users" ifExists="true"/>
    </changeSet>

</databaseChangeLog>
